    "rollup-plugin-node-resolve": "^2.0.0",
    "zuul": "latest"
  },
  "keywords": [
    "test",
    "bdd",
    "assert",
    "should"
  ],
  "main": "./index.js",
  "license": "MIT",
  "dependencies": {
    "should-equal": "^1.0.0",
    "should-format": "^3.0.2",
    "should-type": "^1.4.0",
    "should-type-adaptors": "^1.0.1",
    "should-util": "^1.0.0"
  },
